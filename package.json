{
  "name": "kinofan",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "node --env-file=.env pre-build.ts && next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn start",
    "ts-check": "tsc --noEmit",
    "knip": "knip",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier:fix && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}' --cache --cache-strategy content",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei --cache --cache-strategy content",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier:fix": "prettier --write '**/*.{ts,tsx,md,json,yml}' --cache --cache-strategy metadata src",
    "test:commit": "commitlint -e",
    "biome:check": "biome format --write ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@effector/next": "0.7.0",
    "@tanstack/react-query": "5.14.1",
    "@tanstack/react-query-devtools": "5.14.1",
    "classcat": "5.0.4",
    "dayjs": "1.11.10",
    "effector": "23.1.0",
    "effector-react": "23.1.0",
    "next": "14.0.4",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.2",
    "react-hot-toast": "2.4.1",
    "rooks": "7.14.1",
    "sharp": "0.33.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@neodx/svg": "0.6.1",
    "@next/bundle-analyzer": "14.0.4",
    "@tanstack/eslint-plugin-query": "5.12.1",
    "@types/node": "20.10.5",
    "@types/nprogress": "0.2.3",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.12.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "knip": "3.8.3",
    "lint-staged": "15.2.0",
    "oxlint": "0.0.21",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "stylelint": "16.0.2",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-standard": "35.0.0",
    "stylelint-order": "6.0.4",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  }
}
