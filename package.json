{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn start",
    "test:all": "yarn tsc --noEmit && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn tsc --noEmit && yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint './src/**/*.{ts,tsx}'",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint './src/**/*.css'",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier": "prettier --write './src/**/*.{ts,tsx,json,md}'",
    "test:commit": "commitlint -e",
    "generate-sprite": "yarn sprite --group --root 'public/svg-sprite-assets' -o 'public/imgs/svg-sprites' -d 'src/types/icon.ts'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
