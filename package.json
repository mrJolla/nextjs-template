{
  "name": "kinofan",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "node --env-file=.env pre-build.ts && next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn start",
    "ts-check": "tsc --noEmit",
    "knip": "knip",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier:fix && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}' --cache --cache-strategy content",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei --cache --cache-strategy content",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier:fix": "prettier --write '**/*.{ts,tsx,md,json,yml}' --cache --cache-strategy metadata src",
    "test:commit": "commitlint -e",
    "biome:check": "biome format --write ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@effector/next": "0.7.0",
    "@tanstack/react-query": "5.56.2",
    "@tanstack/react-query-devtools": "5.58.0",
    "classcat": "5.0.5",
    "dayjs": "1.11.13",
    "effector": "23.2.2",
    "effector-react": "23.2.1",
    "next": "14.2.13",
    "nprogress": "0.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "rooks": "7.14.1",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@neodx/svg": "0.7.1",
    "@next/bundle-analyzer": "14.2.13",
    "@tanstack/eslint-plugin-query": "5.58.1",
    "@types/node": "22.7.4",
    "@types/nprogress": "0.2.3",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.14.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "eslint-plugin-unicorn": "55.0.0",
    "eslint-plugin-unused-imports": "4.0.1",
    "husky": "9.1.6",
    "knip": "5.30.6",
    "lint-staged": "15.2.10",
    "oxlint": "0.9.9",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "stylelint": "16.9.0",
    "stylelint-config-clean-order": "6.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2"
  }
}
